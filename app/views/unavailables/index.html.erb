<p id="notice"><%= notice %></p>

<h1>Listing Unavailables</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @unavailables.each do |unavailable| %>
      <tr>
        <td><%= unavailable.user %></td>
        <td><%= unavailable.date %></td>
        <td><%= link_to 'Show', unavailable %></td>
        <td><%= link_to 'Edit', edit_unavailable_path(unavailable) %></td>
        <td><%= link_to 'Destroy', unavailable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Unavailable', new_unavailable_path %>
